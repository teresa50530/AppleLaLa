@model MVCDataLibrary.DB_Model.Customer_coupon

@{
    ViewBag.Title = "Delete";
}

@section topCSS {
    <link href="~/Assets/css/tables.css" rel="stylesheet" />
}

<h2>刪除客戶優惠券</h2>

<h3>確定要刪除?</h3>
<div>
    <hr />
    <dl class="dl-horizontal horizontal">
        <dt>
            優惠券名稱
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Coupon.Coupon_name)
        </dd>

        <dt>
            帳戶
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Customer.Account)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions">
            <input type="submit" value="確認刪除" class="btn del_btn confirm_del" />
        </div>
    }

    <div>
        @Html.ActionLink("回到列表", "Index")
    </div>
</div>
